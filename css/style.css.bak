*{margin:0;padding:0;border:0;}
header{
	width:1000px;
	height:168px;
	margin:0 auto;
	position:relative;
}
.header-left{
	width:141px;
	height:120px;
	background:url('../images/drlogo.png');
	position:absolute;
	top:32px;
	left:0px;
}
.header-right{
	width:580px;
	height:120px;
	position:absolute;
	top:32px;
	right:0px;
}
.header-right-top{
	width:580px;
	height:40px;
}
.header-right-top ul{
	height:40px;
	font-size:12px;
	list-style:none;
	float:left;
}
.header-right-top ul li{
	line-height:30px;
	float:left;
}
.header-right-top ul li a{
	padding-left:15px;
	padding-right:10px;
	text-decoration:none;
	color:#999;
	display:inline-block;
}
.join{
	background:#d9b39b;
	color:#fff;
}
.tel{
	padding-left:35px;
	background:url('../images/telleft.png') no-repeat;
	background-position:15px 8px;
}
.phone{
	padding-left:35px;
	background:url('../images/03.png') no-repeat;
	background-position:20px 8px;
}
.phone:hover{
	color:#d9b39b;
}
.phone:hover .app{
	display:block;
}
.app{
	position:absolute;
	top:32px;
	left:200px;
	display:none;
	z-index:2;
}
.word{
	padding-left:25px;
	padding-right:20px;
	background:url('../images/09.png') no-repeat;
	background-position:96px 12px;
}
.word1{
	width:50px;
	height:30px;
	position:absolute;
	top:50px;
	left:23px;
	display:none;
}
.word1 ul {
    color: #000;
    width: 75px;
	position:absolute;
	top:-22px;
	left:310px;
}
.word1 li {
    list-style: none;
    text-align: center;
    height: 20px;
	color:#666;
}
.word a:hover .word1{
	display:block;
}
.city{
	padding-left:15px;
}
.header-right-bottom{
	width:580px;
	height:80px;
	background:url('../images/drz.png') no-repeat;
	background-position:140px 30px;
	position:relative;
}
.header-right-bottom p{
	display:inline-block;
	position:absolute;
	font-size:14px;
	bottom:12px;
	left:135px;
	padding-right:15px;
	background:url('../images/09.png') no-repeat;
	background-position:60px 6px;
}
.header-right-bottom input{
	border:1px solid #999;
	width:200px;
	height:20px;
	position:absolute;
	bottom:8px;
	left:212px;
	outline:none;
}
.header-right-bottom a{
	border:1px solid #999;
	width:80px;
	height:20px;
	line-height:20px;
	text-align:center;
	position:absolute;
	text-decoration:none;
	font-size:12px;
	bottom:8px;
	left:430px;
}
.header-right-bottom a:hover{
	background:#eee;
}



