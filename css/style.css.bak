	#index-banner{
		position: relative;
		width: 100%;
		margin: 0 auto;
		height: 400px;
		min-width: 1000px;
		z-index:-2;
	}
	#index-banner img{
		position: absolute;
		top: 0;
		/* left:-300px; */
		left: 50%;
		margin-left: -960px;  
	}
	#index-banner a{
	    position: absolute;
		top: 0;
		width: 100%;
		height: 400px;
		overflow: hidden;
		text-decoration: none;
		outline: none;
	}


